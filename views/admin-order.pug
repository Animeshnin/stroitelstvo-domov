h1 Admin-panel
h2 Admin-order
table
    tr
        th id
        th Время заказа
        th Номер пользователя 
        th Номер товара
        th Цена
        th Пользователь
        th Номер телефона
        th Адресс
    each val, index in oreder
        tr
            td #{val['id']}
            td #{val['human_date']}
            td #{val['user_id']}
            td #{val['goods_id']}
            td #{val['goods_cost'].toFixed(2).replace(/\d(?=(\d{3})+\.)/g, '$& ')} руб
            td #{val['user']}
            td #{val['phone']}
            td #{val['address']}